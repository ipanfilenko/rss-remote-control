{
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "airbnb-base",
      "airbnb-typescript/base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "import/prefer-default-export": "off",
      "no-await-in-loop": "off",
      "consistent-return": "off",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ]
    },
    "ignorePatterns": []
  }
  